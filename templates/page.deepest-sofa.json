        "0ed3107d-ba79-4720-b2db-3b6ff2bd2887": {
          "type": "review",
          "settings": {
            "text": "Fast delivery, Great Service, Amazing Couch. Overall an amazing experience! Will be ordering another sofa in the coming months for other rooms in my house.",
            "title": "JEFF J (SANTA ANA, CA)",
            "image_1": "shopify://shop_images/819646cb-7afa-47e7-b5e2-1fd3719abc8e_large_e3b19ca5-fee0-47a7-abe6-f3b8f04e7bde.jpg",
