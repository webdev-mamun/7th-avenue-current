          "1582236442347": {
            "type": "image_and_text",
            "settings": {
              "bg_color": "#b2c2e0",
              "image_position": "image_right",
              "title": "Why Grafiti",
              "image": "shopify://shop_images/vignette_1_5ba8a774-138e-4227-a69f-35320ed0ce86.jpg",
              "col_title": "Dedicated Discount for<br>Trade Professionals",
              "col_text": "Industry leading trade discounts on all products for accepted trade professionals. Program is available to eligible interior designers, architects, home builders, home stagers, and more.\n\nNo minimum orders"
