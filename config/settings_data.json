          "1582154908477": {
            "type": "faqs",
            "settings": {
              "question_cat": "order-and-shipping",
              "title": "How long is the turnaround time? How long until I receive my product?",
              "answer": "We aim to ship every order out within 3 business days. We send every order via standard shipping."
