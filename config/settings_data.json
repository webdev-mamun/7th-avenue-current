      "advanced-content": {
        "type": "advanced-content",
        "blocks": {
          "1584040414909": {
            "type": "image-with-icon-and-text",
            "settings": {
              "title": "Premium Mahogany <span>Wood Frames</span>",
              "svg1": "fsc-black.svg",
              "text1": "Most wall art frames are made of styrofoam. Not ours. We use FSC-certified & handcrafted mahogany wood frames.",
              "svg2": "cotton-black.svg",
              "text2": "Our fine art canvases are 100% cotton and comes from a mill in France with over 150 years of crafting history.",
              "svg3": "frame-black.svg",
              "text3": "Our canvases are hand mounted on premium quality, solid-wood stretcher bars.",
              "block_width": 85,
              "image_position": "right",
              "image_position_on_mobile": "top"
            }
          },
          "1584040333393": {
            "type": "image-with-text",
            "settings": {
              "title": "Handcrafted in <span>Los Angeles</span>",
              "text": "<p>Each piece is produced and framed by hand by our team of experts with decades of combined craftsmanship experience.</p>\n\n<p>We use professional-grade fine art materials and Giclée printing techniques to protect and preserve the integrity of the artwork. Each piece is then coated with a fine art glaze that enriches colors, creates a water-resistant barrier, and prevents cracking.</p>",
              "block_width": 85,
              "image_position": "left",
              "image_position_on_mobile": "top"
            }
          }
        },
