<!doctype html>
<html lang="en" class="no-js">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta name="theme-color" content="{{ settings.button_color }}">
    <link rel="canonical" href="{{ canonical_url }}">

    <!-- Preconnect ================================================== -->
    {% # theme-check-disable %}
    <link rel="preconnect" href="https://fonts.shopifycdn.com" crossorigin>
    <link rel="preconnect" href="https://cdn.shopify.com" crossorigin>
    <link rel="preconnect" href="https://cdnjs.cloudflare.com" crossorigin>
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link rel="preconnect dns-prefetch" href="https://triplewhale-pixel.web.app/" crossorigin>
    <link rel="preconnect dns-prefetch" href="https://api.config-security.com/" crossorigin>
    {% # theme-check-enable %}

    {%- if settings.favicon != blank -%}
      <link rel="shortcut icon" href="{{ settings.favicon | image_url: width: 32, height: 32 }}" type="image/png">
    {%- endif -%}

    <!-- SEO ================= -->
    {%- render 'seo-title' -%}

    {%- if page_description -%}
      <meta name="description" content="{{ page_description | escape }}">
    {%- endif -%}

    {%- if request.path contains 'shipping-returns' or template == 'product.not-for-sale' -%}
      <meta name="robots" content="noindex">
    {%- endif -%}

    {%- render 'social-meta-tags' -%}

    <!-- preload CSS================================================== -->
    {% # theme-check-disable %}
    <link
      rel="preload"
      as="style"
      href="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.14.0/themes/base/jquery-ui.min.css"
      crossorigin="anonymous"
      onload="this.onload=null;this.rel='stylesheet'"
    >
    <link
      rel="preload"
      as="style"
      href="https://cdnjs.cloudflare.com/ajax/libs/slim-select/2.9.2/slimselect.css"
      crossorigin="anonymous"
      onload="this.onload=null;this.rel='stylesheet'"
    >

    <link
      rel="preload"
      as="style"
      href="{{ 'okendo-reviews.css' | asset_url }}"
      crossorigin="anonymous"
      onload="this.onload=null;this.rel='stylesheet'"
    >
    <link
      rel="preload"
      as="style"
      href="{{ 'theme.css' | asset_url }}"
      crossorigin="anonymous"
      onload="this.onload=null;this.rel='stylesheet'"
    >

    <!-- preload JS================================================== -->
    <link
      rel="preload"
      as="script"
      href="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.7.1/jquery.min.js"
      crossorigin="anonymous"
    >
    <link
      rel="preload"
      as="script"
      href="https://cdnjs.cloudflare.com/ajax/libs/slim-select/2.9.2/slimselect.min.js"
      crossorigin="anonymous"
    >
    {% # theme-check-enable %}

    <!-- FontFace ============== -->
    {%- render 'font-face' -%}
    <!-- CSSVariables ========== -->
    {%- render 'css-variables' -%}
    <!-- CSSInclude ============ -->
    {%- render 'css-include' -%}
    <!-- JSVariables =========== -->
    {%- render 'js-variables' -%}

    <!-- ThirdPartyHeaderScripts ============= -->
    {%- render 'third-party-header-scripts' -%}

    {{ content_for_header }}

    <!-- ThemeJS ============= -->
    {% # theme-check-disable %}
    <script
      src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.7.1/jquery.min.js"
      crossorigin="anonymous"
      defer
    ></script>
    <script
      src="https://cdnjs.cloudflare.com/ajax/libs/jqueryui/1.14.0/jquery-ui.min.js"
      crossorigin="anonymous"
      defer
    ></script>
    <script
      src="https://cdnjs.cloudflare.com/ajax/libs/slim-select/2.9.2/slimselect.min.js"
      crossorigin="anonymous"
      defer
    ></script>
    <script
      src="{{ 'jquery.fancybox.min.js' | asset_url }}"
      type="text/javascript"
      crossorigin="anonymous"
      defer
    ></script>
    <script src="{{ 'vendor.js' | asset_url }}" type="text/javascript" crossorigin="anonymous" defer></script>
    <script src="{{ 'theme.js' | asset_url }}" type="text/javascript" crossorigin="anonymous" defer></script>
    {% # theme-check-enable %}

    {%- if settings.show_multiple_currencies -%}
      <!-- Currencies =========== -->
      {%- render 'currencies' -%}
    {%- endif -%}

    <!-- AppInclude =========== -->
    {% # theme-check-disable %}
    <script src="https://assets.calendly.com/assets/external/widget.js" type="text/javascript" defer></script>
    {% # theme-check-enable %}

    {%- render 'calendly-page-tracking-code' -%}

    <!-- Customers =========== -->
    {%- if template contains 'customers' -%}
      <script
        src="{{ "shopify_common.js" | shopify_asset_url }}"
        type="text/javascript"
        crossorigin="anonymous"
        defer
      ></script>
      <script src="{{ "customer_area.js"  | shopify_asset_url }}" crossorigin="anonymous" defer></script>
    {%- endif -%}

    <link rel="next" href="/next">
    <link rel="prev" href="/prev">

    <script>
      !function(){window.semaphore=window.semaphore||[],window.ketch=function(){window.semaphore.push(arguments)};var e=document.createElement("script");e.type="text/javascript",e.src="https://global.ketchcdn.com/web/v3/config/7_avenue/ave7th/boot.js",e.defer=e.async=!0,document.getElementsByTagName("head")[0].appendChild(e)}();
    </script>
  </head>

  <body
    class="gridlock shifter-left template-{{ template.name }} {{ template }} {{template.name}}__{{ template.suffix  }} {{template.name}} page__{{ page.handle }} shifter-enabled loading"
    style="--header: 120px;"
  >
    <div class="page_loader" id="page_loader">
      <div class="loader_site_logo">
        <img
          class="spinner"
          src="https://cdn.shopify.com/s/files/1/0602/7195/2115/files/Logo_-_Borders.svg?v=1637126699"
          width="100%"
          height="100%"
          alt="7th avenue"
          loading="lazy"
        >
      </div>
    </div>

    {% if settings.ajax_cart_method == 'drawer' and template != 'cart' %}
      {%- render 'cart-drawer' -%}
    {% endif %}

    <div class="shifter-page" id="PageContainer">
      {% section 'header' %}
      <div class="page-wrap">
        {% if settings.scrolling_bar_position == 'header' %}
          {% section 'scrolling-announcements' %}
        {% endif %}

        {% if template == 'index'
          or template contains 'blog'
          or template contains 'collection'
          or template contains 'page'
          or template contains 'product'
        %}
          {% if template.suffix == 'faqs' %}
            {{ content_for_layout }}
            <div class="faq-container">
              <div class="faq-grid">
                {% section 'faq-side-bar' %}
                <div class="faq-group">
                  {% sections 'faq-group' %}
                </div>
                {% section 'faq-side-bar-right' %}
              </div>
            </div>
          {% else %}
            {{ content_for_layout }}
          {% endif %}
        {% else %}
          <div id="contents">
            {% render 'breadcrumb' %}
            {{ content_for_layout }}
          </div>
        {% endif %}
      </div>

      {% if settings.scrolling_bar_position == 'above_footer' %}
        {% section 'scrolling-announcements' %}
      {% endif %}

      {% section 'footer' %}

      {% if settings.scrolling_bar_position == 'below_footer' %}
        {% section 'scrolling-announcements' %}
      {% endif %}

      {% render 'popup' %}
    </div>
    {% section 'mobile-navigation' %}

    <a href="#" class="scrollup"><i class="la la-chevron-up" aria-hidden="true"></i> TOP</a>

    <div class="fancybox-wrapper"></div>
    <div class="clear"></div>

    {% if template contains 'product' %}
      <script src="{{ '//ajax.aspnetcdn.com/ajax/jquery.templates/beta1/jquery.tmpl.min.js' }}" defer></script>
      <script src="{{ 'jquery.products.min.js' | asset_url }}" defer></script>
    {% endif %}

    <!-- Affirm -->
    <script>
      _affirm_config = {
      public_api_key: "CDCD3CWZ7HVAPCJ2",
      script: "https://cdn1.affirm.com/js/v2/affirm.js",
      session_id: "sID"
      };
      (function(l,g,m,e,a,f,b){var d,c=l[m]||{},h=document.createElement(f),n=document.getElementsByTagName(f)[0],k=function(a,b,c){return function(){a[b]._.push([c,arguments])}};c[e]=k(c,e,"set");d=c[e];c[a]={};c[a]._=[];d._=[];c[a][b]=k(c,a,b);a=0;for(b="set add save post open empty reset on off trigger ready setProduct".split(" ");a<b.length;a++)d[b[a]]=k(c,e,b[a]);a=0;for(b=["get","token","url","items"];a<b.length;a++)d[b[a]]=function(){};h.async=!0;h.src=g[f];n.parentNode.insertBefore(h,n);delete g[f];d(g);l[m]=c})(window,_affirm_config,"affirm","checkout","ui","script","ready");
    </script>
    <!-- End Affirm -->

    <script>
      window.ALBSS = {event_key: 'a1909e85-5426-4b25-8117-953038d0fe05'};
    </script>
    <script src="https://c.albss.com/p/l/loader.iife.js" async></script>

    <!-- sh-ssp-added -->
    {% render 'sh-ssp-theme-snippet' %}
    <!-- /sh-ssp-added -->

    <!-- Google AdWords Dynamic Remarketing -->
    {%- render 'adwords-dynamic-remarketing' -%}
  </body>
</html>
