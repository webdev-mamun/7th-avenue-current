<div class="quality-cus">
  <div class="cus-left">
  	<img class="cus-img" src="{{ section.settings.image_q | img_url: 'original' }}" width="100%" height="100%" loading="lazy">
  </div>
  <div class="cus-right">
  <h1 class="cus-qu">{{ section.settings.section_text }}</h1>
  <p class="cus-text">{{ section.settings.text }} </p>
  </div>
</div>


{% schema %}
  {
    "name": "Our Quality",
    "settings": [
	{
       "type":"text",
       "id":"section_text",
       "label":"Section Heading"
     },
	{
        "type": "image_picker",
        "id": "image_q",
        "label": "LeftImage"
      },
	{
        "type": "richtext",
        "id": "text",
        "label": "Text"
      }
	]
  }
{% endschema %}

