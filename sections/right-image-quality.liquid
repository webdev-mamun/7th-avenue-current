<section class="img-left-cus-sec">
  <div class="left-img-f-sce">
    <h2> {{ section.settings.title }} </h2>
    <p> {{ section.settings.text }} </p>
  </div>
  <div class="img-right-r-sce">
    <img src="{{ section.settings.image_r | img_url: 'original' }}" width="100%" height="100%" loading="lazy">
  </div>
</section>
{% schema %}
  {
    "name": "Right Image quality",
    "settings": [
	{
        "type": "text",
        "id": "title",
        "label": "Heading"
      },
      {
        "type": "richtext",
        "id": "text",
        "label": "Text"
      },
		{
        "type": "image_picker",
        "id": "image_r",
        "label": "Image"
      }
	]
  }
{% endschema %}
